NmsPDU DEFINITIONS IMPLICIT TAGS ::=
-- Use the DEFINITIONS EXPLICIT TAGS in your definition file.
-- * define tags in CHOICE constructs
-- * do not define tags in SEQUENCE constructs
-- * do not use SET constructs

BEGIN

IMPORTS 
    SupercastPDU    FROM ModSupercast
    TrackerPDU      FROM ModTracker
    LocatorPDU      FROM ModLocator;

PDU ::= CHOICE {
    modSupercastPDU [0] SupercastPDU,
    modTrackerPDU   [2] TrackerPDU,
    modLocatorPDU   [3] LocatorPDU
}

END
